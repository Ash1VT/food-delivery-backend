{
  "name": "order",
  "version": "1.0.0",
  "author": "ash1vt",
  "license": "MIT",
  "scripts": {
    "env:dev": "dotenv -e ./env/.env.dev",
    "env:test": "dotenv -e ./env/.env.test",
    "build": "tsc && yarn build:paths",
    "build:paths": "tscpaths -p tsconfig.json -s . -o dist",
    "watch": "tsc-watch --onSuccess \"yarn build:paths\"",
    "start": "node ./dist/src/app.js",
    "start:dev": "yarn env:dev -- -- nodemon --inspect ./dist/src/app.js",
    "test": "yarn env:test -- -- jest -c jest.config.json",
    "test:coverage": "yarn env:test -- -- jest --coverage -c jest.config.json",
    "migrate:dev": "yarn env:dev -- -- prisma migrate dev",
    "migrate:test": "yarn env:test -- -- prisma migrate dev",
    "admin:dev": "yarn env:dev -- -- prisma studio"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "@prisma/client": "^5.8.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.7.0",
    "moment": "^2.30.1",
    "nodemon": "^3.0.2",
    "openapi-types": "^12.1.3",
    "prisma": "^5.8.0",
    "prisma-factory": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "tscpaths": "^0.0.9",
    "typescript": "^5.3.3"
  }
}
