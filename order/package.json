{
  "name": "order",
  "version": "1.0.0",
  "main": "index.js",
  "author": "ash1vt",
  "license": "MIT",
  "scripts": {
    "build": "tsc && yarn build:paths",
    "build:paths": "tscpaths -p tsconfig.json -s . -o dist",
    "start": "node ./dist/src/index.js",
    "watch": "tsc -w",
    "start:dev": "dotenv -e ./env/.env.dev -- nodemon --inspect ./dist/src/index.js",
    "start:test": "dotenv -e ./env/.env.test -- node ./dist/tests/index.js",
    "migrate:dev": "dotenv -e ./env/.env.dev -- prisma migrate dev",
    "migrate:test": "dotenv -e ./env/.env.test -- prisma migrate dev"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@prisma/client": "^5.8.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.8.0",
    "ts-node": "^10.9.2",
    "tscpaths": "^0.0.9",
    "typescript": "^5.3.3"
  }
}
